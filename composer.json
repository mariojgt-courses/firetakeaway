{
    "name": "mariojgt/firetakeaway",
    "description": "A Laravel Package Firetakeaway, a quick start for new laravel Packages",
    "keywords": [
        "laravel"
    ],
    "type": "laravel",
    "license": "MIT",
    "authors": [
        {
            "name": "mario",
            "email": "mariojgt2@gmail.com"
        }
    ],
    "require":{
        "php": "^7.3|^8.0",
        "laravel/sanctum": "^2.9",
        "intervention/image": "^2.5"
    },
    "autoload": {
        "psr-4": {
            "Mariojgt\\Firetakeaway\\": "src"
        }
    },

    "scripts": {
        "post-autoload-dump": [
            "@php artisan route:list"
        ]
    },

    "extra": {
        "laravel": {
            "providers": [
                "Mariojgt\\Firetakeaway\\FiretakeawayProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
